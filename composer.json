{
  "name": "wexample/symfony-loader-testing",
  "description": "A dynamic rendering system for Symfony",
  "type": "library",
  "license": "MIT",
  "version": "0.0.1",
  "keywords": [
    "symfony-ux"
  ],
  "autoload": {
    "psr-4": {
      "Wexample\\SymfonyLoader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wexample\\SymfonyLoader\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "weeger",
      "email": "romain.weeger@wexample.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "wexample/symfony-loader": "0.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/twig-bundle": "^7.4",
    "wexample/symfony-testing": "1.0.61",
    "doctrine/doctrine-bundle": "^2.18",
    "doctrine/orm": "^3.5",
    "symfony/browser-kit": "^7.4"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../*",
      "options": {
        "symlink": true
      }
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "[ -f bin/console ] && php bin/console loader:check-node-install || echo 'Skipping console command (no Symfony app)'"
    ],
    "post-update-cmd": [
      "[ -f bin/console ] && php bin/console loader:check-node-install || echo 'Skipping console command (no Symfony app)'"
    ]
  }
}
